{
  "name": "wampus",
  "version": "0.3.0",
  "description": "WAMP client for JavaScript",
  "main": "index.js",
  "repository": "https://github.com/GregRos/wampus",
  "author": "GregRos",
  "license": "MIT",
  "keywords": [
    "wamp",
    "rpc",
    "promise",
    "esnext",
    "typescript",
    "rxjs"
  ],
  "private": false,
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "tsc -b",
    "build:clean": "npm-run-all clean build",
    "test:unit": "nyc ava --config ava.unit.js",
    "test:integration": "ava --config ava.integration.js",
    "test:all": "nyc ava --config ava.all.js",
    "test": "yarn run test:all",
    "docs": "node dist/tools/run.js",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "package:build": "npm-run-all lint build:clean test && node dist/tools/publish.js",
    "package:publish": "yarn run package:build && (cd .publish; yarn publish)",
    "prereq:router": "./test-scripts/install-router.sh"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^12.11.7",
    "@types/shelljs": "^0.8.5",
    "@types/sinon": "^7.5.1",
    "@types/string-template": "^1.0.2",
    "@types/ws": "^5.1.2",
    "ava": "^2.4.0",
    "codecov": "^3.1.0",
    "event-target-shim": "^5.0.1",
    "globby": "^10.0.1",
    "mock-socket": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "retarget-sourcemaps-after-move": "^0.2.0",
    "rewiremock": "^3.13.9",
    "rxjs": "^6.5.4",
    "shelljs": "^0.8.3",
    "sinon": "^9.0.0",
    "source-map-support": "^0.5.6",
    "transcurse": "^0.2.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-config-gregros": "^1.6.0",
    "typed-wamp": "^0.2.1",
    "typedoc": "^0.15.6",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-internal-external": "^2.0.2",
    "typescript": "^3.7.4",
    "ws-mock": "^0.1.0"
  },
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.15",
    "objectology": "^0.0.5",
    "ws": "^5.2.1"
  },
  "peerDependencies": {
    "rxjs": "^6.3.3",
    "transcurse": "^0.2.0",
    "typed-wamp": "^0.1.0"
  },
  "nyc": {
    "produce-source-map": true,
    "include": [
      "dist/lib/**/*.js",
      "src/lib/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true
  },
  "typings": "index"
}
