{
  "name": "wampus",
  "version": "0.3.0",
  "description": "WAMP client for JavaScript",
  "main": "index.js",
  "repository": "https://github.com/GregRos/wampus",
  "author": "GregRos",
  "license": "MIT",
  "keywords": [
    "wamp",
    "rpc",
    "promise",
    "esnext",
    "typescript",
    "rxjs"
  ],
  "private": false,
  "scripts": {
    "build:clean": "rm -rf dist/",
    "build": "tsc",
    "build:fresh": "npm-run-all build:clean build",
    "test:unit": "nyc ava --config ava.unit.js",
    "test:integration": "ava --config ava.integration.js",
    "test": "npm-run-all build:fresh test:unit test:integration",
    "docs": "node dist/generate-docs/run.js",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "package:publish": "yarn run build && node dist/publish.js && (cd .tmp/publish; yarn publish)"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.149",
    "@types/mz": "^0.0.32",
    "@types/node": "^12.11.7",
    "@types/shelljs": "^0.8.5",
    "@types/string-template": "^1.0.2",
    "@types/ws": "^5.1.2",
    "ava": "^2.4.0",
    "codecov": "^3.1.0",
    "globby": "^10.0.1",
    "mz": "^2.7.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "retarget-sourcemaps-after-move": "^0.2.0",
    "rxjs": "^6.3.3",
    "shelljs": "^0.8.3",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.6",
    "tslint": "^5.20.1",
    "tslint-config-gregros": "^1.2.1",
    "typedoc": "^0.15.6",
    "typedoc-plugin-example-tag": "^1.0.2",
    "typedoc-plugin-external-module-name": "^2.1.0",
    "typedoc-plugin-internal-external": "^2.0.2",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "isomorphic-ws": "^4.0.1",
    "lodash": "^4.17.15",
    "objectology": "^0.0.5",
    "promise-stuff": "^0.3.6",
    "string-template": "^1.0.0",
    "typed-wamp": "^0.1.0",
    "ws": "^5.2.1"
  },
  "peerDependencies": {
    "rxjs": "^6.3.3",
    "typed-wamp": "^0.1.0"
  },
  "nyc": {
    "produce-source-map": true,
    "include": [
      "dist/lib/**/*.js",
      "src/lib/**/*.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": true
  },
  "typings": "index"
}
