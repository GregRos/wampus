{
  "extends": [
    "tslint-config-gregros"
  ],
  "rules": {
    "no-implicit-dependencies": false,
    "completed-docs": {
      "severity": "warn",
      "options": {
        "interfaces": {
          "visibilities": [
            "exported"
          ]
        },
        "classes": {
          "visibilities": [
            "exported"
          ]
        },
        "namespaces": {
          "visibilities": [
            "exported"
          ]
        },
        "types": {
          "visibilities": [
            "exported"
          ]
        },
        "enums": {
          "visibilities": [
            "exported"
          ]
        }
      }
    }
  }
}
