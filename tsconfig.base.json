{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "sourceMap": true,
    "noImplicitThis": true,
    "allowUnreachableCode": true,
    "lib": [
      "es6",
      "es2016.array.include",
      "dom"
    ],
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node"
  }
}