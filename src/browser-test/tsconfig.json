{
  "extends": "../../tsconfig.base",
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es2018",
    "outDir": "../../dist/browser-test",
    "paths": {
      "~lib/*": [
        "../lib/*"
      ],
      "~client-side/*": [
        "./*"
      ]
    },
    "noImplicitThis": true,
    "declaration": false
  },
  "references": [{
    "path": "../lib"
  }],
  "include": [
    "**/*.ts",
    "**/*.json"
  ]
}